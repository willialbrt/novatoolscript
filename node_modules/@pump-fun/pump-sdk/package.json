{
  "name": "@pump-fun/pump-sdk",
  "version": "1.3.8",
  "description": "Pump Bonding Curve SDK",
  "keywords": [],
  "homepage": "https://github.com/pump-fun/pump-sdk#readme",
  "bugs": {
    "url": "https://github.com/pump-fun/pump-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pump-fun/pump-sdk.git"
  },
  "license": "MIT",
  "author": "pump-fun",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "require": "./dist/*/index.js",
      "import": "./dist/*/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@pump-fun/pump-swap-sdk": "^0.0.1-beta.36",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "bn.js": "^5.2.2",
    "undici-types": "^6.20.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/bn.js": "^5.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.7.0",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ],
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "devnet",
        "prerelease": true
      }
    ]
  }
}
